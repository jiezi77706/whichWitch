    "relay:start": "node scripts/start-cybergraph-relay.js",
    "test:ai": "node scripts/test-ai-features.js",
    "clean-install": "node scripts/clean-install.js",
    "postinstall": "echo '✅ 依赖安装完成！使用 npm run dev:full 启动项目'"
  },
